<% if  logged_in? %>
    <h1>タスク一覧</h1>
    <ul>
        <% @tasks.each do |task| %>
        <li>
            <%= link_to task.id, task %>
            <%= task.content %>
            &gt;
            <%= task.status %>
        </li>
        <% end %>
    </ul>
    <%= link_to '新規Taskの登録', new_task_path %>
    <%= link_to 'ログアウト', logout_path, method: :delete %>
<% else %>
    <%= link_to '新規ユーザの登録', signup_path %>
    <%= link_to 'ログイン', login_path %>
<% end %>